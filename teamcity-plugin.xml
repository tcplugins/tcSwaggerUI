<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>tcSwaggerUI</name>
    <display-name>SwaggerUI for REST APIs</display-name>
    <version>@Version@</version>
    <description>Provides a UI for exploring the REST API.</description>
    <download-url>https://github.com/tcplugins/tcSwaggerUI/releases</download-url>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
      <logo>https://raw.githubusercontent.com/tcplugins/tcSwaggerUI/master/docs/icons/tcSwaggerUI.png</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true"/>
</teamcity-plugin>
